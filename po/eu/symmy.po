msgid "Your names"
msgstr "Iñigo Salvador Azurmendi"

#, kde-format
msgctxt "EMAIL OF TRANSLATORS"
msgid "Your emails"
msgstr "xalba@ni.eus"

#: plugins/decryptfileitemaction.cpp:31
#, kde-format
msgctxt "@action:inmenu Decrypt action in Dolphin context menu"
msgid "Decrypt"
msgstr "Deszifratu"

#: plugins/encryptfileitemaction.cpp:42
#, kde-format
msgctxt "@action:inmenu Encrypt action in Dolphin context menu"
msgid "Encrypt"
msgstr "Zifratu"

#: src/compositejob.cpp:58
#, kde-kuit-format
msgctxt "@info"
msgid ""
"Encryption operation failed. Please check whether the <application>gpg-"
"agent</application> process is running."
msgstr ""
"Zifratzeko eragiketak huts egin du. Mesedez, egiaztatu <application>gpg-"
"agent</application> prozesua martxan dagoen."

#: src/compositejob.cpp:84
#, kde-kuit-format
msgctxt "@info"
msgid ""
"Decryption operation failed. Please check whether the decryption key is "
"correct.<nl/>You should also check whether the <application>gpg-agent</"
"application> process is running."
msgstr ""
"Deszifratzeko eragiketak huts egin du. Mesedez, egiaztatu deszifratzeko "
"gakoa zuzena den.<nl/><application>gpg-agent</application> prozesua martxan "
"dagoen ere egiaztatu beharko zenuke."

#: src/compositejob.cpp:87
#, kde-kuit-format
msgctxt "@info"
msgid ""
"Could not decrypt the following ciphertexts.<nl/>Please check whether the "
"decryption key is correct."
msgstr ""
"Ezin du ondoko zifratutako testua deszifratu.<nl/>Mesedez, egiaztatu "
"deszifratzeko gakoa zuzena den."

#: src/compositejob.cpp:127
#, kde-format
msgid "Asking Passphrase"
msgstr "Pasahitz eskatzea"

#: src/compositejob.cpp:131
#, kde-format
msgid "Please supply a password or passphrase to be used as encryption key."
msgstr ""
"Mesedez, eman zifratzeko gako gisa erabiltzeko pasahitz edo pasaesaldi bat."

#: src/compositejob.cpp:136
#, kde-format
msgid "Please supply a password or passphrase to be used as decryption key."
msgstr ""
"Mesedez, eman deszifratzeko gako gisa erabiltzeko pasahitz edo pasaesaldi "
"bat."

#: src/compositejob.cpp:157
#, kde-format
msgctxt "description of an encryption job"
msgid "Encrypting"
msgstr "Zifratzea"

#: src/compositejob.cpp:158
#, kde-format
msgctxt "File used as input of the encryption algorithm"
msgid "Plaintext"
msgstr "Testu arrunta"

#: src/compositejob.cpp:159
#, kde-format
msgctxt "File created by the encryption algorithm"
msgid "Ciphertext"
msgstr "Zifratutako testua"

#: src/compositejob.cpp:161
#, kde-format
msgctxt "description of a decryption job"
msgid "Decrypting"
msgstr "Deszifratzea"

#: src/compositejob.cpp:162
#, kde-format
msgctxt "File used as input of the decryption algorithm"
msgid "Ciphertext"
msgstr "Zifratutako testua"

#: src/compositejob.cpp:163
#, kde-format
msgctxt "File created by the decryption algorithm"
msgid "Plaintext"
msgstr "Testu arrunta"

#: src/main.cpp:27
#, kde-format
msgctxt "display name for 'symmy' binary"
msgid "GPG Symmetric Encryption Frontend"
msgstr "GPGren zifratze simetrikoaren aurrealdekoa"

#: src/main.cpp:28
#, kde-format
msgid "Encrypt/decrypt one ore more files using GPG symmetric encryption."
msgstr ""
"Zifratu/deszifratu fitxategi bat edo gehiago GPGren zifratze simetrikoa "
"erabiliz."

#: src/main.cpp:28
#, kde-format
msgid "(c) 2017 Elvis Angelaccio"
msgstr "(c) 2017 Elvis Angelaccio"

#: src/main.cpp:29
#, kde-format
msgid "Elvis Angelaccio"
msgstr "Elvis Angelaccio"

#: src/main.cpp:33
#, kde-format
msgid "List of files to encrypt or decrypt."
msgstr "Zifratu edo deszifratu beharreko fitxategien zerrenda."

#: src/main.cpp:35
#, kde-format
msgid ""
"Encrypt the given list of files (this is the default if there are no other "
"options)."
msgstr ""
"Zifratu emandako fitxategi-zerrenda (hau lehenetsitakoa da beste aukerarik "
"ez badago)."

#: src/main.cpp:37
#, kde-format
msgid "Decrypt the given list of files."
msgstr "Deszifratu emandako fitxategien-zerrenda."

#: src/main.cpp:46
#, kde-format
msgctxt "--encrypt and --decrypt are CLI args, don't translate them"
msgid "Error: either --encrypt or --decrypt, not both."
msgstr "Errorea: «--encrypt» edo «--decrypt», baina ez biak batera."
