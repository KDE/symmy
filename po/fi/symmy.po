msgid "Your names"
msgstr "Tommi Nieminen"

#, kde-format
msgctxt "EMAIL OF TRANSLATORS"
msgid "Your emails"
msgstr "translator@legisign.org"

#: plugins/decryptfileitemaction.cpp:31
#, kde-format
msgctxt "@action:inmenu Decrypt action in Dolphin context menu"
msgid "Decrypt"
msgstr "Pura salaus"

#: plugins/encryptfileitemaction.cpp:42
#, kde-format
msgctxt "@action:inmenu Encrypt action in Dolphin context menu"
msgid "Encrypt"
msgstr "Salaa"

#: src/compositejob.cpp:58
#, kde-kuit-format
msgctxt "@info"
msgid ""
"Encryption operation failed. Please check whether the <application>gpg-"
"agent</application> process is running."
msgstr ""
"Salaus epäonnistui. Tarkista, onko <application>gpg-agent</application>-"
"prosessi käynnissä."

#: src/compositejob.cpp:84
#, kde-kuit-format
msgctxt "@info"
msgid ""
"Decryption operation failed. Please check whether the decryption key is "
"correct.<nl/>You should also check whether the <application>gpg-agent</"
"application> process is running."
msgstr ""
"Salauksen purku epäonnistui. Tarkista, että salausavain on oikea.<nl/"
">Tarkista myös, onko <application>gpg-agent</application>-prosessi käynnissä."

#: src/compositejob.cpp:87
#, kde-kuit-format
msgctxt "@info"
msgid ""
"Could not decrypt the following ciphertexts.<nl/>Please check whether the "
"decryption key is correct."
msgstr ""
"Seuraavia salaviestejä ei voitu purkaa.<nl/>Tarkista, että salausavain on "
"oikea."

#: src/compositejob.cpp:127
#, kde-format
msgid "Asking Passphrase"
msgstr "Kysytään salasanaa"

#: src/compositejob.cpp:131
#, kde-format
msgid "Please supply a password or passphrase to be used as encryption key."
msgstr "Anna salausavaimena käytettävä salasana tai -lause."

#: src/compositejob.cpp:136
#, kde-format
msgid "Please supply a password or passphrase to be used as decryption key."
msgstr "Anna salausavaimena käytettävä salasana tai -lause."

#: src/compositejob.cpp:157
#, kde-format
msgctxt "description of an encryption job"
msgid "Encrypting"
msgstr "Salataan"

#: src/compositejob.cpp:158
#, kde-format
msgctxt "File used as input of the encryption algorithm"
msgid "Plaintext"
msgstr "Paljas teksti"

#: src/compositejob.cpp:159
#, kde-format
msgctxt "File created by the encryption algorithm"
msgid "Ciphertext"
msgstr "Salattu teksti"

#: src/compositejob.cpp:161
#, kde-format
msgctxt "description of a decryption job"
msgid "Decrypting"
msgstr "Puretaan"

#: src/compositejob.cpp:162
#, kde-format
msgctxt "File used as input of the decryption algorithm"
msgid "Ciphertext"
msgstr "Salattu teksti"

#: src/compositejob.cpp:163
#, kde-format
msgctxt "File created by the decryption algorithm"
msgid "Plaintext"
msgstr "Paljas teksti"

#: src/main.cpp:25
#, kde-format
msgctxt "display name for 'symmy' binary"
msgid "GPG Symmetric Encryption Frontend"
msgstr "Symmetrisen GPG-salauksen käyttöliittymä"

#: src/main.cpp:26
#, kde-format
msgid "Encrypt/decrypt one ore more files using GPG symmetric encryption."
msgstr ""
"Salaa tai pura yhden tai useamman tiedoston salaus symmetristä GPG-salausta "
"käyttäen."

#: src/main.cpp:26
#, kde-format
